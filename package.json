{
  "name": "@axlair/jupyterlab_vim",
  "version": "0.13.0",
  "description": "Code cell vim bindings",
  "author": "Axel Fahy",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/axelfahy/jupyterlab-vim",
  "bugs": {
    "url": "https://github.com/axelfahy/jupyterlab-vim/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axelfahy/jupyterlab-vim.git"
  },
  "keywords": [
    "extension",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "tslint",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": ">=3.0.0-rc.5",
    "@jupyterlab/cells": ">=3.0.0-rc.5",
    "@jupyterlab/codemirror": ">=3.0.0-rc.5",
    "@jupyterlab/notebook": ">=3.0.0-rc.5",
    "@lumino/commands": "^1.10.1",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/domutils": "^1.1.7",
    "@types/codemirror": "^0.0.87"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "tslint": "^6.1.2",
    "@jupyterlab/builder": "^3.0.0-rc.2",
    "typescript": "~4.0.3"
  }
}
